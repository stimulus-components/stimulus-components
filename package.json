{
  "private": true,
  "scripts": {
    "lint": "pnpm lint:check && pnpm lint:js && pnpm lint:prettier",
    "lint:check": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.mjs,.mts,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
    "docs": "pnpm -C docs dev",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo": "^8.0.4",
    "@nuxt/eslint-config": "^0.2.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "^9.29.0",
    "np": "9.2.0",
    "postcss": "8.4.37",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.2.0",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@9.12.2"
}
